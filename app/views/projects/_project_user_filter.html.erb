<h2>Users</h2><% reset_cycle %><% all_label = "All Users" %><% selected = params[:users].nil? || params[:users].empty? %><div class="user <%= cycle("odd", "even") %> <%= selected ? "selected" : "deselected" %> ">		<div class="toggle left">		<%= image_tag('user_check_null.png') %>			</div>	<div class="avatar left">		<%= image_tag("/users/avatars/default/tiny.png", {:title => all_label})%>	</div>	<div class="details">		<div class="name">			<%= link_to all_label,  {:controller=>'projects', :action=>'toggle', :users=>@filtered_types.join(',')}, :remote=>true %>		</div>		<div class="summary">			<%= @all_projects.length %> projects		</div>	</div>	<div class="clear"></div></div><hr /><% User.all.each do |user| %>
<%= render :partial => 'project_user_filter_row', :locals=> {:user => user } %>
<% end -%>