<%= render(:partial => 'shared/form_errors', :locals => {:object => @user})%>	<table summary="User form fields" >		<tr>		<th>Given Name</th>		<td><%= f.text_field(:first_name)%></td>	</tr>	<tr>		<th>Surname</th>		<td><%= f.text_field(:last_name)%></td>	</tr>	<tr>		<th>UserName</th>		<td><%= f.text_field(:username)%></td>	</tr>		<tr>		<th>Email Address</th>		<td><%= f.text_field(:email)%></td>	</tr>	<tr>		<th>Job Title</th>		<td><%= f.text_field(:title)%></td>	</tr>		<tr>		<th>Description</th>		<td><%= f.text_field(:description)%></td>	</tr>	<tr>		<th>User Level</th>		<td><%= f.select(:level, UserRole.all.collect {|r| [r.title, r.id]}) %></td>	</tr>		<tr>		<th>avatar</th>		<td> 		<% if @user.avatar %>			<%= image_tag @user.avatar.url(:thumb) %> 			<%= f.check_box :del_avatar %> 		<% end %>		<%= f.file_field :avatar %></td>	</tr>		<tr>		<th>Password</th>		<td><%= f.password_field 'supplied_password' %></td>	</tr>	</table>