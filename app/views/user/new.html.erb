<%= link_to("<< Back to list", {:action => 'list'}, :class => "back-link") %>
<div class="user new">
	<h2>Create New User</h2>
	
	<%= form_for(:user, :url => {:action => 'create'}, :html => { :multipart => true }) do |f| %>
	
	<%= render(:partial => 'form', :locals => {:f => f})%>
	
	<div class="form-buttons">
		<%= submit_tag("Create User") %>
	</div>
	
	<% end %>
</div>

