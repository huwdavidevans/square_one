<%= link_to("<< Back to list", {:action => 'list'}, :class => "back-link") %>
<div class="user edit">
	<h2>Edit User</h2>	
	<%= form_for(:user, :url => {:action => 'update', :id=>@user.id}, :html => { :multipart => true } ) do |f| %>	
	<%= render(:partial => 'form', :locals => {:f => f})%>	
	<div class="form-buttons">
		<%= submit_tag("Update User") %>
	</div>
	
	<% end %>
</div>
