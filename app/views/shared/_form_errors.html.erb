<% if object.errors.any? %>	<% content_for :notify do %>	<div id="error_explanation">		<h2><%= pluralize(object.errors.count, "error") %> prohibited this task from being saved:</h2>		<div class="fixed">			<ul>				<% object.errors.full_messages.each do |msg| %>				<li>					<%= msg %>				</li>				<% end %>			</ul>		</div>	</div>	<% end %><% end %>